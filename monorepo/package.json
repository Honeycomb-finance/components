{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:app": "turbo run dev:app --concurrency=20",
    "dev:packages": "turbo run dev --concurrency=20",
    "start:app": "turbo run start --filter=example-app",
    "build:packages": "turbo run build --filter=@pangolindex/* --filter=@honeycomb/*",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@pangolindex/sdk": "5.3.5-internal.7d84e42",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "patch-package": "6.4.7",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "tsc-alias": "1.8.7"
  },
  "peerDependencies": {
    "@pangolindex/sdk": ">=2.0.1"
  }
}
